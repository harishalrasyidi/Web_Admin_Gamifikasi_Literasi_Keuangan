				{
					"name": "Google Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// 1. Ambil respon JSON dari API\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"// 2. Cek di console (opsional, untuk debugging)\r",
									"console.log(jsonData);\r",
									"\r",
									"// 3. Simpan token ke Collection Variable\r",
									"// PENTING: Ganti 'access_token' sesuai dengan key di respon JSON Anda\r",
									"// Jika responnya { \"token\": \"...\" }, ganti jadi jsonData.token\r",
									"pm.collectionVariables.set(\"access_token\", jsonData.access_token);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Accept",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"google_id_token\": \"eyJhGciOq1\",\n  \"platform\": \"Android\",\n  \"locale\": \"id_ID\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/auth/google",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"auth",
